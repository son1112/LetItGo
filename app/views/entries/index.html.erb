<div class="text-box">
  <%= render 'form_min' %>
</div>

<% @entries.reverse.each do |entry| %>
  <div id="entry">
    <h1 class="entry-title"><%= entry.title %></h1>
    <h2 class="entry-subtitle"><%= entry.subtitle %></h2> 
    <p class="entry-content"><%= entry.body %></p>
  </div>
<% end %>
<br />



